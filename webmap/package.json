{
  "name": "CUF-webpack",
  "version": "0.0.1",
  "description": "Lazy load AngularJS modules using Webpack and AngularUI Router",
  "scripts": {
    "start": "hs build",
    "build": "cross-env NODE_ENV=production webpack -p",
    "devs": "webpack-dev-server --hot --inline"
  },
  "dependencies": {
    "jquery": "2.1.4",
    "maphilight": "1.2.3",
    "angular": "1.5.11",
    "angular-route": "~1.5.0",
    "angular-tooltips": "1.2.2"
  },
  "devDependencies": {
    "babel-core": "5.8.38",
    "babel-loader": "5.4.2",
    "css-loader": "0.23.1",
    "file-loader": "0.8.5",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "copy-webpack-plugin": "4.0.1",
    "json-loader": "0.5.4",
    "ng-annotate-webpack-plugin": "0.1.3",
    "node-libs-browser": "0.5.3",
    "node-sass": "3.8.0",
    "expose-loader": "0.7.1",
    "sass-loader": "4.0.0",
    "style-loader": "0.13.1",
    "svg-sprite-loader": "0.0.22",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  }
}
